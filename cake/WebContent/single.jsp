	function addToCart()
	{
		var cakeid="${cake.cake_id}";
		 var num = document.getElementById('quantity').value;
		 $.ajax({
			    async:true, 
		        type: "POST",  
		        url: "${ctx}/cart.do",//注意路径  
		        data:{cakeid:cakeid,count:num},  
		        dataType:"json", 					      
		        success:function(data){ 				        						           		            
		            	 alert("添加购物车成功");
		            	 window.location.href="${ctx}/products";
		            },					        
		        error:function(data){
		        	alert("添加购物车失败，失败原因【" + data + "】");
		        }					       
		    });  					  
	}
</script>
